    <div class="row center-block" style="width:1186px;">
        <div class="col-sm-12"><p style="color:white;font-size:160%;">Autopilot</p></div>
    </div>
    <div class="row center-block" style="width:1186px;">
        <div class="col-sm-2"><img height="150" width="150" src="~/Images/Project Images/Project Autopilot.png" /></div>
        <div class="col-sm-8"><p style="color:#d2d2d2; font-size:160%;">This project brings high level Artificial Intelligence to aircraft pilots.</p></div>
    </div>
    <div class="row center-block" style="height:20px;"><p> </p></div>
    <!--Lower Navbar-->
    @Html.Partial("LowerNavbarFixed")

